<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <ImageView
        android:id="@+id/bubble1"
        android:layout_width="100dp"
        android:layout_height="110dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="104dp"
        android:layout_toEndOf="@+id/bubble2"
        android:contentDescription="@null"
        android:src="@drawable/bubl1" />

    <ImageView
        android:id="@+id/bubble2"
        android:layout_width="151dp"
        android:layout_height="146dp"
        android:layout_alignStart="@+id/bubble7"
        android:layout_below="@+id/bubble7"
        android:contentDescription="@null"
        android:src="@drawable/bubl2" />

    <ImageView
        android:id="@+id/bubble3"
        android:layout_width="126dp"
        android:layout_height="136dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="24dp"
        android:layout_marginStart="74dp"
        android:contentDescription="@null"
        android:src="@drawable/bubl3" />

    <ImageView
        android:id="@+id/bubble6"
        android:layout_width="154dp"
        android:layout_height="152dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="109dp"
        android:contentDescription="@null"
        android:src="@drawable/bubl4" />

    <ImageView
        android:id="@+id/bubble7"
        android:layout_width="153dp"
        android:layout_height="134dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="36dp"
        android:layout_marginTop="34dp"
        android:contentDescription="@null"
        android:src="@drawable/bubl5"
        android:onClick="TimeMangmet"
        />

    <ImageView
        android:id="@+id/logo"
        android:layout_width="104dp"
        android:layout_height="123dp"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/bubble3"
        android:contentDescription="@null"
        android:src="@drawable/logo" />
</RelativeLayout>